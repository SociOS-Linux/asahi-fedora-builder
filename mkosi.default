[Distribution]
Distribution=fedora
Release=36
Architecture=x86_64
UseHostRepositories=yes
Repositories=fedora,updates,asahi-linux, socios-linux

[Output]
Format=directory
Output=mkosi.rootfs
Bootable=no
Hostname=fedora

[Packages]
Packages=
    uboot
    uboot-asahi
    update-m1n1
    audit
    bash-completion
    btrfs-progs
    cloud-utils-growpart
    dhcp-client
    diffutils
    dosfstools
    dnf
    dnf-plugins-core
    e2fsprogs
    efivar
    findutils
    grub2-efi-x86_64
    grub2-efi-x86_64-modules
    grubby
    hostname
    iproute
    iputils
    iwd
    kernel
    kmod
    linux-firmware
    net-tools
    m1n1
    m1n1-tools
    man-db
    man-pages
    ncurses
    openssh-clients
    openssh-server
    passwd
    pciutils
    rootfiles
    rpm
    rsync
    sudo
    symlinks
    systemd-networkd
    tar
    usbutils
    vim-minimal
    vim-enhanced
    wget

[Validation]
Password=fedora
